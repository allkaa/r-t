{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch Project",
          "program": "${workspaceFolder}/EventEmitterSample.js",
          "cwd": "${workspaceFolder}"
      },
      {
          "type": "node",
          "request":"launch",
          "name": "Launch File",
          "program": "${file}",
          "cwd":"${workspaceFolder}",
      },
      {
        "type": "node",
        "request":"launch",
        "name": "Launch --experimental-modules File",
        "program": "${file}",
        "cwd":"${workspaceFolder}",
        "runtimeArgs": ["--experimental-modules"]
    },
    {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome localhost:3000",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "sourceMapPathOverrides": {"webpack:///src/*": "${webRoot}/*"}
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome localhost:5000",
        "url": "http://localhost:5000",
        "webRoot": "${workspaceFolder}/build",
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome ./build/index.html",
        "file": "${workspaceFolder}/build/index.html"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome ./StyleSheetsDemo.html",
        "file": "${workspaceFolder}/StyleSheetsDemo.html"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome ./StyleSheetsDemoGood.html",
        "file": "${workspaceFolder}/StyleSheetsDemoGood.html"
      }
  ]
}